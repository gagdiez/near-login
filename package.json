{
  "name": "tutorial-react-parcel-express",
  "version": "1.0.0",
  "description": "A tiny getting started for a react project, with front-end built with parcel, and served through express.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:web && npm run move:files",
    "build:web": "cd web && npm run build",
    "move:files": "rm -rf dist && mv ./web/dist ./dist",
    "start": "cd ./server && npm run start",
    "postinstall": "cd server && npm install"
  },
  "keywords": [
    "tutorial",
    "near",
    "parcel",
    "express"
  ],
  "author": "gagdiez",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "connect-flash": "^0.1.1",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-autosanitizer": "^1.0.2",
    "express-session": "^1.17.3",
    "mocha": "~10.1.0",
    "nunjucks": "^3.2.3",
    "near-api-js": "^1.1.0",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "should": "~13.2.3"
  }
}